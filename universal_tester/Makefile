dist: 
	@mkdir -p .obj/tester && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/tester/ && $(COPY_FILE) --parents images/usb.jpg images/serial.jpg images/serial-os.jpg .obj/tester/ && $(COPY_FILE) --parents qtaircablemainform.ui.h .obj/tester/ && ( cd `dirname .obj/tester` && $(TAR) tester.tar tester && $(GZIP) tester.tar ) && $(MOVE) `dirname .obj/tester`/tester.tar.gz . && $(DEL_FILE) -r .obj/tester

